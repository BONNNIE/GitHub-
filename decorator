import time
    
def decorator(function):
    def wrapTheFunction(*args,**kwargs):
                t1=time.time()
                function(*args,**kwargs)
                t2=time.time()
                print("Using Time:",t2-t1,"\n")
    return wrapTheFunction

@decorator
def S():
    sum=0
    for i in range(1,10000000):
        if i%3==0 or i%5==0:
            sum+=i
    print(sum)
S()
